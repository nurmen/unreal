%\transblindshorizontal
\MyLogo
\vspace{1.0cm}
\begin{beamercolorbox}[sep=1.0cm, center, shadow=false, rounded=true]{postit2}
\begin{Huge}Grazie per l'attenzione\end{Huge}
\end{beamercolorbox}
\pause
\end{frame}

