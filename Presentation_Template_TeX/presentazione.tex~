\end{frame}

\begin{frame}{La pompa di calore}
\framesubtitle{funzionamento in raffrescamento (ciclo a compressione di vapore)}
%\transdissolve
\MyLogo
\begin{columns}
\column{.25\textwidth}
\begin{align*}
 \alert{COP_C} &= \dfrac{Q_1}{\abs{L}} \\
