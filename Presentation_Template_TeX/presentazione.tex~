%\begin{beamercolorbox}[sep=0.1cm, wd=.5\textwidth, center, shadow=false, rounded=true]{postit}
%mfmdfmdfmd
%\end{beamercolorbox} 
%%% FINE ESEMPIO DEF POSTIT
%\end{center}
%\column{.5\textwidth}
%\end{columns}
\end{frame}

